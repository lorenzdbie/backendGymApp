{
  "name": "fitnessapp",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "jest": {
    "testPathIgnorePatterns": [
      "src",
      "node_modules",
      "config"
    ]
  },
  "scripts": {
    "start": "npx env-cmd nodemon",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "npx env-cmd -f .env.test jest --coverage",
    "lint": "npx eslint . --fix"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.4.2",
    "@koa/router": "^12.0.0",
    "config": "^3.3.8",
    "env-cmd": "^10.1.0",
    "jest": "^29.3.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa2-swagger-ui": "^5.6.0",
    "mysql2": "^2.3.3",
    "node-emoji": "^1.11.0",
    "serialize-error": "^11.0.0",
    "supertest": "^6.3.1",
    "swagger-jsdoc": "^6.2.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20"
  }
}
